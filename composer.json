{
    "name": "digital-creative/nova-pill-filter",
    "description": "A Laravel Nova filter that renders into clickable pills.",
    "keywords": [
        "laravel",
        "nova",
        "filter",
        "pill",
        "tags"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\PillFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\PillFilter\\PillFilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
